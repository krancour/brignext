{
  "id": "brignext-demo",
  "description": "A project that demonstrates brignext",
  "workerConfigs": {
    "myCLIWorker": {
      "events": [
        {
          "provider": "brignext-cli",
          "types": ["exec"]
        }
      ],
      "image": {
        "repository": "brigadecore/brigade-worker",
        "tag": "v1.2.1",
        "pullPolicy": "IfNotPresent"
      }
    },
    "myGitHubWorker": {
      "events": [
        {
          "provider": "github",
          "types": ["push"]
        }
      ]
    },
    "myAlwaysWorker": {}
  }
}
